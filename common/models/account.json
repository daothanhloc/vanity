{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "accountName": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": "fromUserId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "histories": {
      "type": "hasMany",
      "model": "History",
      "foreignKey": "",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "effectScripts": {
      "type": "hasMany",
      "model": "EffectScript",
      "foreignKey": "",
      "through": "PayEffectScript"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
